<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.biancheng.mapper.StudentCardMapper">

    <select id="selectStudentCardById" parameterType="Integer" resultType="com.biancheng.po.StudentCard">
        select id,studentId,startDate,endDate from studentCard where id = #{id}
    </select>

</mapper>